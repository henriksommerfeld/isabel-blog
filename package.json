{
  "name": "gatsby-starter-netlify-cms",
  "description": "Example Gatsby, and Netlify CMS project",
  "version": "1.1.3",
  "author": "Austin Green",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start:app": "npm run develop",
    "start": "run-p start:**",
    "build:app": "npm run clean && gatsby build",
    "build": "run-p build:**",
    "develop": "npm run clean && gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "npx tsc",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "codegen": "graphql-codegen --config graphql-codegen.yml"
  },
  "dependencies": {
    "gatsby": "^2.13.51",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-netlify": "^2.1.3",
    "gatsby-plugin-netlify-cms": "^4.1.6",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-remark-copy-linked-files": "^2.1.4",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.1.8",
    "gatsby-transformer-remark": "^2.6.10",
    "gatsby-transformer-sharp": "^2.2.5",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms-app": "^2.9.7",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/introspection": "^1.5.0",
    "@graphql-codegen/typescript": "^1.5.0",
    "@graphql-codegen/typescript-operations": "^1.5.0",
    "@types/graphql": "^14.2.3",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.7.1",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "http-proxy-middleware": "^0.19.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  }
}
