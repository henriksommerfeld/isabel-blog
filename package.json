{
  "name": "gatsby-starter-netlify-cms",
  "description": "Example Gatsby, and Netlify CMS project",
  "version": "1.1.3",
  "author": "Austin Green",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "npm run build:plugin && gatsby build",
    "build:plugin": "cd plugins/gatsby-source-twitter-unfurl && yarn && cd ../..",
    "develop": "npm run clean && nodemon --exec \"gatsby develop --host=0.0.0.0\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "serve": "gatsby serve --host=0.0.0.0",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "type-check": "npx tsc",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "upgrade-packages": "yarn upgrade-interactive && npx syncyarnlock"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "dependencies": {
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.3.0",
    "@raae/gatsby-remark-oembed": "^0.1.1",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.35",
    "gatsby-background-image": "^0.8.15",
    "gatsby-image": "^2.2.27",
    "gatsby-plugin-catch-links": "^2.1.13",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-netlify": "^2.1.19",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "^4.1.23",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-scroll-indicator": "^0.1.4",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-plugin-styled-components": "^3.1.9",
    "gatsby-plugin-typescript": "^2.1.12",
    "gatsby-plugin-typography": "^2.3.12",
    "gatsby-remark-copy-linked-files": "^2.1.26",
    "gatsby-remark-embed-video": "^2.0.0",
    "gatsby-remark-images": "^3.1.26",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.23",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-source-twitter": "^3.0.0",
    "gatsby-transformer-remark": "^2.6.28",
    "gatsby-transformer-sharp": "^2.2.21",
    "intersection-observer": "^0.7.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms-app": "^2.9.7",
    "netlify-cms-backend-fs": "^0.4.4",
    "nodemon": "^1.19.3",
    "p-map": "^3.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-dom-confetti": "^0.1.1",
    "react-helmet": "^5.2.1",
    "react-ripples": "^2.2.1",
    "react-spring": "^8.0.27",
    "react-swipeable": "^5.4.0",
    "react-typography": "^0.16.19",
    "reactn": "^2.2.4",
    "styled-components": "^4.4.0",
    "typography": "^0.16.19",
    "typography-theme-alton": "^0.16.19",
    "use-onclickoutside": "^0.3.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/introspection": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@graphql-codegen/typescript-operations": "^1.8.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-helmet": "^5.0.11",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.18",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "http-proxy-middleware": "^0.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2",
    "remark": "^11.0.1",
    "source-map-support": "^0.5.13",
    "strip-markdown": "^3.1.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
