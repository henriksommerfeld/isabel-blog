{
  "name": "gatsby-starter-netlify-cms",
  "description": "Example Gatsby, and Netlify CMS project",
  "version": "1.1.3",
  "author": "Austin Green",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "gatsby build",
    "develop": "npm run clean && nodemon --exec \"gatsby develop --host=0.0.0.0\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "type-check": "npx tsc",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "upgrade-packages": "yarn upgrade-interactive && npx syncyarnlock"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "dependencies": {
    "@raae/gatsby-remark-oembed": "^0.1.1",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.2",
    "gatsby-background-image": "^0.8.9",
    "gatsby-image": "^2.2.16",
    "gatsby-plugin-netlify": "^2.1.11",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "^4.1.13",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-sharp": "^2.2.19",
    "gatsby-plugin-styled-components": "^3.1.4",
    "gatsby-plugin-typescript": "^2.1.6",
    "gatsby-plugin-typography": "^2.3.6",
    "gatsby-remark-copy-linked-files": "^2.1.14",
    "gatsby-remark-embed-video": "^1.7.1",
    "gatsby-remark-images": "^3.1.20",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.11",
    "gatsby-remark-video": "^1.2.3",
    "gatsby-source-filesystem": "^2.1.19",
    "gatsby-transformer-remark": "^2.6.20",
    "gatsby-transformer-sharp": "^2.2.13",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms-app": "^2.9.7",
    "netlify-cms-backend-fs": "^0.4.4",
    "nodemon": "^1.19.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-spring": "^8.0.27",
    "react-typography": "^0.16.19",
    "styled-components": "^4.3.2",
    "typography": "^0.16.19",
    "typography-theme-alton": "^0.16.19",
    "use-onclickoutside": "^0.3.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/introspection": "^1.7.0",
    "@graphql-codegen/typescript": "^1.7.0",
    "@graphql-codegen/typescript-operations": "^1.7.0",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.3",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.12",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "http-proxy-middleware": "^0.19.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "typescript": "^3.6.2"
  }
}
