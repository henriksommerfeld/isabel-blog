{
  "name": "isabel-blog",
  "description": "Blog of Isabel Sommerfeld built with GatsbyJS and Netlify CMS",
  "version": "1.0.0",
  "author": "Henrik Sommerfeld",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run build:plugin && npm run clean && nodemon --exec \"gatsby develop --host=0.0.0.0\"",
    "build": "npm run build:plugin && gatsby build",
    "build:plugin": "cd plugins/gatsby-source-twitter-unfurl && yarn && cd ../..",
    "develop": "gatsby develop",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "serve": "gatsby serve --host=0.0.0.0",
    "format": "prettier --write \"{gatsby-*.js,src/**/*.js}\"",
    "test:unit": "jest",
    "test:unit:watch": "jest --watchAll",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:record": "cypress run --record --key 098f2f3b-90f0-4a00-b668-7686f454b8cc",
    "test:cypress:ci": "start-server-and-test develop http://localhost:8000 test:cypress:record",
    "cy": "start-server-and-test develop http://localhost:8000 test:cypress:open",
    "cy:cli": "start-server-and-test develop http://localhost:8000 test:cypress",
    "lint": "eslint . --ext js,ts,jsx,tsx",
    "lint:fix": "yarn lint --fix",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "pre-commit": "npm run format && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henriksommerfeld/isabel-blog.git"
  },
  "resolutions": {
    "cypress": "4.3.0"
  },
  "dependencies": {
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.3.0",
    "@raae/gatsby-remark-oembed": "0.1.1",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.150",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "babel-plugin-styled-components": "1.10.7",
    "babel-preset-gatsby": "^0.4.1",
    "gatsby": "2.21.24",
    "gatsby-background-image": "1.1.1",
    "gatsby-image": "2.4.3",
    "gatsby-plugin-catch-links": "2.3.1",
    "gatsby-plugin-disqus": "1.1.4",
    "gatsby-plugin-google-analytics": "2.3.1",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-netlify": "2.3.2",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-netlify-cms": "4.1.31",
    "gatsby-plugin-offline": "3.2.1",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-scroll-indicator": "1.0.0",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-plugin-styled-components": "3.3.1",
    "gatsby-plugin-typescript": "2.4.2",
    "gatsby-plugin-typography": "2.5.1",
    "gatsby-remark-copy-linked-files": "2.3.2",
    "gatsby-remark-embed-video": "3.0.10",
    "gatsby-remark-images": "3.3.2",
    "gatsby-remark-relative-images": "0.3.0",
    "gatsby-remark-responsive-iframe": "2.4.2",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-transformer-remark": "2.8.7",
    "gatsby-transformer-sharp": "2.5.2",
    "http-proxy-middleware": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "intersection-observer": "0.10.0",
    "js-yaml": "3.13.1",
    "lodash": "4.17.15",
    "lodash-webpack-plugin": "0.11.5",
    "netlify-cms-backend-fs": "0.4.4",
    "netlify-cms-app": "2.9.7",
    "p-map": "4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-dom-confetti": "0.1.3",
    "react-helmet": "6.0.0",
    "react-ripples": "2.2.1",
    "react-spring": "8.0.27",
    "react-swipeable": "5.5.1",
    "react-typography": "0.16.19",
    "reactn": "2.2.7",
    "remark": "^12.0.0",
    "source-map-support": "^0.5.19",
    "strip-markdown": "^3.1.2",
    "styled-components": "5.1.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "typography": "0.16.19",
    "typography-theme-alton": "0.16.19",
    "use-onclickoutside": "0.3.1",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.5",
    "@graphql-codegen/introspection": "1.13.5",
    "@graphql-codegen/typescript": "1.13.5",
    "@graphql-codegen/typescript-operations": "1.13.5",
    "@testing-library/cypress": "6.0.0",
    "@types/jest": "25.2.1",
    "@types/node": "14.0.1",
    "@types/react-helmet": "6.0.0",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "babel-jest": "25.3.0",
    "cypress": "4.3.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "husky": "^4.2.5",
    "jest": "25.3.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "start-server-and-test": "1.11.0",
    "ts-jest": "25.5.1"
  }
}