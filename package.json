{
  "name": "isabel-blog",
  "description": "Blog of Isabel Sommerfeld built with GatsbyJS and Netlify CMS",
  "version": "1.0.0",
  "author": "Henrik Sommerfeld",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run build:plugin && npm run clean && nodemon --exec \"gatsby develop --host=0.0.0.0\"",
    "build": "npm run build:plugin && gatsby build",
    "build:plugin": "cd plugins/gatsby-source-twitter-unfurl && yarn && cd ../..",
    "develop": "gatsby develop",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "serve": "gatsby serve --host=0.0.0.0",
    "format": "prettier --trailing-comma es5 --semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test:unit": "jest",
    "test:unit:watch": "jest --watchAll",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:record": "cypress run --record --key 098f2f3b-90f0-4a00-b668-7686f454b8cc",
    "test:cypress:ci": "start-server-and-test develop http://localhost:8000 test:cypress:record",
    "cy": "start-server-and-test develop http://localhost:8000 test:cypress:open",
    "cy:cli": "start-server-and-test develop http://localhost:8000 test:cypress",
    "type-check": "npx tsc",
    "lint": "eslint --ignore-path .gitignore . --ext tsx",
    "lint:fix": "yarn lint --fix",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "upgrade-packages": "yarn upgrade-interactive && npx syncyarnlock"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henriksommerfeld/isabel-blog.git"
  },
  "dependencies": {
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.3.0",
    "@raae/gatsby-remark-oembed": "^0.1.1",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.16.3",
    "gatsby-background-image": "^0.8.15",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-catch-links": "^2.1.15",
    "gatsby-plugin-disqus": "^1.1.2",
    "gatsby-plugin-google-analytics": "^2.1.23",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-netlify": "^2.1.22",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "^4.1.25",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-scroll-indicator": "^0.1.4",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-plugin-typescript": "^2.1.15",
    "gatsby-plugin-typography": "^2.3.14",
    "gatsby-remark-copy-linked-files": "^2.1.28",
    "gatsby-remark-embed-video": "^2.0.0",
    "gatsby-remark-images": "^3.1.28",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.25",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-source-twitter": "^3.0.0",
    "gatsby-transformer-remark": "^2.6.30",
    "gatsby-transformer-sharp": "^2.2.23",
    "intersection-observer": "^0.7.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "netlify-cms-app": "^2.9.7",
    "netlify-cms-backend-fs": "^0.4.4",
    "nodemon": "^1.19.4",
    "p-map": "^3.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-dom-confetti": "^0.1.1",
    "react-helmet": "^5.2.1",
    "react-ripples": "^2.2.1",
    "react-spring": "^8.0.27",
    "react-swipeable": "^5.4.0",
    "react-typography": "^0.16.19",
    "reactn": "^2.2.4",
    "styled-components": "^4.4.0",
    "typography": "^0.16.19",
    "typography-theme-alton": "^0.16.19",
    "use-onclickoutside": "^0.3.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/introspection": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@graphql-codegen/typescript-operations": "^1.8.1",
    "@testing-library/cypress": "^5.0.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.11.1",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-helmet": "^5.0.12",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.20",
    "cypress": "^3.4.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "http-proxy-middleware": "^0.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2",
    "remark": "^11.0.1",
    "source-map-support": "^0.5.13",
    "start-server-and-test": "^1.10.6",
    "strip-markdown": "^3.1.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "wait-on": "^3.3.0"
  }
}
