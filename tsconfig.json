{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "baseUrl": "./src",
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "noUnusedParameters": true
  },
  "exclude": ["node_modules", "public", ".cache", "auto-generated"]
}
